{
  "name": "",
  "description": "通知栏组件，用于显示滚动消息，支持配置消息类型、消息列表、滚动间隔、滚动持续时间、滚动方向、步进滚动、前置图标和后置图标等。",
  "attributes": [
    {
      "name": "type",
      "description": "消息类型",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: 'info'\r\n\t\t\t}"
    },
    {
      "name": "barList",
      "description": "消息列表",
      "type": "{\r\n\t\t\t\ttype: Array as PropType<Array<string>>,\r\n\t\t\t\tdefault: () : Array<string> => [] as Array<string>\r\n\t\t\t}"
    },
    {
      "name": "interval",
      "description": "滚动间隔，单位为毫秒",
      "type": "{\r\n\t\t\t\ttype: Number,\r\n\t\t\t\tdefault: 100\r\n\t\t\t}"
    },
    {
      "name": "duration",
      "description": "滚动持续时间，单位为毫秒",
      "type": "{\r\n\t\t\t\ttype: Number,\r\n\t\t\t\tdefault: 3000\r\n\t\t\t}"
    },
    {
      "name": "vertical",
      "description": "是否纵向滚动",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: false\r\n\t\t\t}"
    },
    {
      "name": "step",
      "description": "是否步进滚动",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: false\r\n\t\t\t}"
    },
    {
      "name": "prefixIcon",
      "description": "前置图标的名称",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: ''\r\n\t\t\t}"
    },
    {
      "name": "prefixClass",
      "description": "前置图标的额外类名",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: ''\r\n\t\t\t}"
    },
    {
      "name": "suffixIcon",
      "description": "后置图标的名称",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: ''\r\n\t\t\t}"
    },
    {
      "name": "suffixIconClass",
      "description": "后置图标的额外类名",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: ''\r\n\t\t\t}"
    }
  ]
}