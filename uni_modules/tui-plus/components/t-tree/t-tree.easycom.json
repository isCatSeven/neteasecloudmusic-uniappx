    },
    {
      "name": "load",
      "description": "加载函数",
      "type": "{\r\n\t\t\t\ttype: Function as PropType<TuiTreeLoad>,\r\n\t\t\t\tdefault: (_ : string) : Promise<TuiTreeData[]> => {\r\n\t\t\t\t\treturn Promise.resolve([] as TuiTreeData[])\r\n\t\t\t\t}\r\n\t\t\t}"
    },
    {
      "name": "showCheckbox",
      "description": "是否显示复选框",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: false\r\n\t\t\t}"
    },
    {
      "name": "lazy",
      "description": "是否懒加载",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: false\r\n\t\t\t}"
    },
    {
      "name": "showEditButton",
      "description": "是否显示编辑按钮",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: false\r\n\t\t\t}"
    },
    {
      "name": "showDeleteButton",
      "description": "是否显示删除按钮",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: false\r\n\t\t\t}"
    }
  ]
