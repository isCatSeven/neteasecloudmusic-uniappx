{
  "name": "",
  "description": "页面组件，用于定义页面的布局和结构，支持配置导航栏、标签栏、加载状态、加载时间、页面模式等。",
  "attributes": [
    {
      "name": "[event]initFinished",
      "description": "",
      "type": "Event"
    },
    {
      "name": "navbar",
      "description": "是否显示导航栏",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: false\r\n\t\t\t}"
    },
    {
      "name": "tabbar",
      "description": "是否显示标签栏",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: true\r\n\t\t\t}"
    },
    {
      "name": "isLoading",
      "description": "是否显示加载状态",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: true\r\n\t\t\t}"
    },
    {
      "name": "loadingTime",
      "description": "加载状态的显示时间，单位为毫秒",
      "type": "{\r\n\t\t\t\ttype: Number,\r\n\t\t\t\tdefault: 100\r\n\t\t\t}"
    },
    {
      "name": "mode",
      "description": "页面模式",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: 'scroll-view'\r\n\t\t\t}"
    },
    {
      "name": "scrollDisabled",
      "description": "是否禁用页面滚动",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: false\r\n\t\t\t}"
    }
  ]
}