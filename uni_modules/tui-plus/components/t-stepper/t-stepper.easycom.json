{
  "name": "",
  "description": "用于在指定的数值范围内进行加减操作，支持多种配置，包括最小值、最大值、步长、按钮样式等。",
  "attributes": [
    {
      "name": "[event]update:modelValue",
      "description": "",
      "type": "Event"
    },
    {
      "name": "[event]change",
      "description": "",
      "type": "Event"
    },
    {
      "name": "modelValue",
      "description": "默认的数值",
      "type": "{\r\n\t\t\t\ttype: Number,\r\n\t\t\t\tdefault: 0\r\n\t\t\t}"
    },
    {
      "name": "min",
      "description": "用户可输入的最小值",
      "type": "{\r\n\t\t\t\ttype: Number,\r\n\t\t\t\tdefault: 0\r\n\t\t\t}"
    },
    {
      "name": "max",
      "description": "用户可输入的最大值",
      "type": "{\r\n\t\t\t\ttype: Number,\r\n\t\t\t\tdefault: 100\r\n\t\t\t}"
    },
    {
      "name": "step",
      "description": "步长，每次加或减的值，支持小数值",
      "type": "{\r\n\t\t\t\ttype: Number,\r\n\t\t\t\tdefault: 1\r\n\t\t\t}"
    },
    {
      "name": "buttonClass",
      "description": "按钮的类名",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: ''\r\n\t\t\t}"
    },
    {
      "name": "inputClass",
      "description": "输入框的类名",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: ''\r\n\t\t\t}"
    },
    {
      "name": "iconMinus",
      "description": "减少图标",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: 'minus'\r\n\t\t\t}"
    },
    {
      "name": "iconPlus",
      "description": "增加图标",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: 'plus'\r\n\t\t\t}"
    },
    {
      "name": "showMinus",
      "description": "是否显示减少按钮",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: true\r\n\t\t\t}"
    },
    {
      "name": "decimalLength",
      "description": "显示的小数位数",
      "type": "{\r\n\t\t\t\ttype: Number,\r\n\t\t\t\tdefault: 0\r\n\t\t\t}"
    },
    {
      "name": "showPlus",
      "description": "是否显示增加按钮",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: true\r\n\t\t\t}"
    },
    {
      "name": "disabledInput",
      "description": "是否禁止输入框",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: false\r\n\t\t\t}"
    },
    {
      "name": "disableMinus",
      "description": "是否禁用减少按钮",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: false\r\n\t\t\t}"
    },
    {
      "name": "disablePlus",
      "description": "是否禁用增加按钮",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: false\r\n\t\t\t}"
    },
    {
      "name": "cursorSpacing",
      "description": "指定光标于键盘的距离，避免键盘遮挡输入框，单位px",
      "type": "{\r\n\t\t\t\ttype: Number,\r\n\t\t\t\tdefault: 100\r\n\t\t\t}"
    },
    {
      "name": "plusInterval",
      "description": "长按加减间隔",
      "type": "{\r\n\t\t\t\ttype: Number,\r\n\t\t\t\tdefault: 100\r\n\t\t\t}"
    },
    {
      "name": "longPress",
      "description": "是否允许长按进行加减",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: true\r\n\t\t\t}"
    }
  ]
}