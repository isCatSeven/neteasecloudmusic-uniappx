{
  "name": "",
  "description": "环形进度条组件，用于显示圆形进度，支持配置线条宽度、激活和未激活颜色、进度值、单位、是否显示标签、过渡持续时间等。",
  "attributes": [
    {
      "name": "lineWidth",
      "description": "进度条线条宽度",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: \"20rpx\"\r\n\t\t\t}"
    },
    {
      "name": "inactiveColor",
      "description": "进度条未激活时的颜色",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: \"#ececec\"\r\n\t\t\t}"
    },
    {
      "name": "activeColor",
      "description": "进度条激活时的颜色",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: \"\"\r\n\t\t\t}"
    },
    {
      "name": "modelValue",
      "description": "进度条的当前值",
      "type": "{\r\n\t\t\t\ttype: Number,\r\n\t\t\t\tdefault: 0\r\n\t\t\t}"
    },
    {
      "name": "unit",
      "description": "进度条值的单位",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: \"%\"\r\n\t\t\t}"
    },
    {
      "name": "showLabel",
      "description": "是否显示进度标签",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: false\r\n\t\t\t}"
    },
    {
      "name": "duration",
      "description": "进度条过渡的持续时间，单位为毫秒",
      "type": "{\r\n\t\t\t\ttype: Number,\r\n\t\t\t\tdefault: 300\r\n\t\t\t}"
    }
  ]
}