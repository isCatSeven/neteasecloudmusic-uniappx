{
  "name": "",
  "description": "TabBar 底部导航，用于创建一个底部标签栏，支持配置标签栏类型、选项数据、激活颜色、未激活颜色、是否有边框、边框类名等。",
  "attributes": [
    {
      "name": "[event]change",
      "description": "",
      "type": "Event"
    },
    {
      "name": "type",
      "description": "标签栏类型",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: 'p'\r\n\t\t\t}"
    },
    {
      "name": "option",
      "description": "选项数据",
      "type": "{\r\n\t\t\t\ttype: Array as PropType<Array<TuiTabBarOption>>,\r\n\t\t\t\tdefault: () : Array<TuiTabBarOption> => [] as Array<TuiTabBarOption>\r\n\t\t\t}"
    },
    {
      "name": "activeColor",
      "description": "激活颜色",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: ''\r\n\t\t\t}"
    },
    {
      "name": "inactiveColor",
      "description": "未激活颜色",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: '#7D7E80'\r\n\t\t\t}"
    },
    {
      "name": "border",
      "description": "是否有边框",
      "type": "{\r\n\t\t\t\ttype: Boolean,\r\n\t\t\t\tdefault: false\r\n\t\t\t}"
    },
    {
      "name": "borderClass",
      "description": "边框类名",
      "type": "{\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: ''\r\n\t\t\t}"
    },
    {
      "name": "badgeAttrs",
      "type": "{\r\n\t\t\t\ttype: Object as PropType<UTSJSONObject>,\r\n\t\t\t\tdefault: () : UTSJSONObject => ({} as UTSJSONObject)\r\n\t\t\t}"
    },
    {
      "name": "current",
      "type": "{\r\n\t\t\t\ttype: Number,\r\n\t\t\t\tdefault: 0\r\n\t\t\t}"
    }
  ]
}